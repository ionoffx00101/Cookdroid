<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:orientation="vertical"
  tools:context="com.coffee.android_20170315_01.Ch08_page23">




  <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
      android:id="@+id/readRawView"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:lines="20"
      android:text="" />
  </LinearLayout>
  <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <Button
      android:id="@+id/btnRawImport"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:text="SD카드에 들어있는 파일 읽기" />
    <Button
      android:id="@+id/btnMkDir"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:text="SD카드에 폴더 만들기" />
    <Button
      android:id="@+id/btnDelDir"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:text="SD카드에 폴더 지우기" />
    <Button
      android:id="@+id/btnMkFile"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:text="SD카드에 파일 만들기" />

    <Button
      android:id="@+id/btnDelFile"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:text="SD카드에 파일 지우기" />

  </LinearLayout>
</LinearLayout>
